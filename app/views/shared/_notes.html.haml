%section#tweeters.clearfix
  - @tweeters.each do |t|
    = link_to(image_tag(t.image), t.url)
  %hr

%section#note_form.clearfix
  = global_validation_message
  = global_success_message
  = render 'shared/note_form'
  %hr

= render 'shared/resources', resources: @blogs, id: 'blogs'

